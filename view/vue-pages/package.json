{
  "name": "vue-pages",
  "version": "0.0.0",
  "type": "module",
  "license":"MIT",
  "scripts": {
    "dev": "vite",
    "watch": "concurrently \"vite build --watch --config script/index.config.ts\" \"vite build --watch --config script/notFound.config.ts\"",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build --config script/index.config.ts && vite build --config script/notFound.config.ts",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "terser": "^5.26.0",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vite-plugin-cdn-import": "^0.3.5",
    "vite-plugin-simple-html": "^0.1.1",
    "vite-plugin-singlefile": "^0.13.5",
    "vue-tsc": "^1.8.25"
  }
}
