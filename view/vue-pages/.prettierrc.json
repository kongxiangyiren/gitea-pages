{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": true,
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "printWidth": 100,
  "trailingComma": "none",
  "vueIndentScriptAndStyle": true,
  "proseWrap": "preserve",
  "arrowParens":"avoid",
  "bracketSpacing":true,
  "endOfLine":"lf",
  "htmlWhitespaceSensitivity":"ignore"
}
